  <script>
    let champMatches = document.querySelectorAll('#matches-tab .collapsible li')
    let spinner = document.querySelector('#matches-tab .spinner')
    let not_found = document.querySelector('#matches-tab .not_found')
    // console.log(champsToday)

    $('#matches-tab .tabs li').on('click', function(){
      let selectTime = $(this).data('tab')
      let currentTime = Date.now();
      let findMatch = false;
      spinner.style.display = 'block'
      not_found.style.display = 'none'

      champMatches.forEach((el,idx,array)=>{
          let diff = el.getAttribute("time")*1000 - currentTime 
          console.log("el",el)
          el.style.display = 'none'

          if (diff > selectTime || diff < 0) {
            el.style.display = 'none'
          } else {
            findMatch = true
            setTimeout(()=>{
              spinner.style.display = 'none'
              el.style.display = 'grid'
            },1000)
          }
        })

        if (!findMatch){
        setTimeout(()=>{
          spinner.style.display = 'none'
          not_found.style.display = 'block'
        },1000)
    }

    })

  </script>

<script>
  let champFixturesMatches = document.querySelectorAll('#fixtures-tab .collapsible li')
  let spinner_fixt = document.querySelector('#fixtures-tab .spinner')
  let not_found_fixt = document.querySelector('#fixtures-tab .not_found')
  // console.log(champsToday)

  $('#fixtures-tab .tabs li').on('click', function(){
    let selectTime = $(this).data('tab')
    let currentDate = new Date().setHours(0, 0, 0, 0) + 86400000;
    let findMatch = false;
    spinner_fixt.style.display = 'block'
    not_found_fixt.style.display = 'none'

    champFixturesMatches.forEach((el,idx,array)=>{
      let diff = currentDate + selectTime - el.getAttribute("time")*1000 
      el.style.display = 'none'

      if (diff < 0 ) {
          el.style.display = 'none'
        } else {
          findMatch = true
          setTimeout(()=>{
            spinner_fixt.style.display = 'none'
            el.style.display = 'grid'
          },1000)
          }
      })

      if (!findMatch){
      setTimeout(()=>{
        spinner_fixt.style.display = 'none'
        not_found_fixt.style.display = 'block'
      },1000)
  }

  })

</script>

<script>
  // var events = document.getElementsByClassName('event');
  let events = document.getElementsByClassName('icon-stats');
  let allStats = document.getElementsByClassName('all-stats');
  let iconStats = document.querySelectorAll('.iconStats>.material-icons');
  let leagueId = '<%= leagueId %>';
  let country = '<%= country %>';
  for(var i = 0; i < iconStats.length; i++)
    iconStats[i].onclick = function() {
      let matchId = this.getAttribute('linkId')
      location.assign(`/${country}/${leagueId}/match?id=${matchId}`)
    }

  for(var i = 0; i < allStats.length; i++)
    allStats[i].onclick = function() {
      let parent = this.closest('.section');
      let matchId = parent.id
      location.assign(`/${country}/${leagueId}/match?id=${matchId}`)
    }

  for(var i = 0; i < events.length; i++)
    events[i].onclick = function() {
      let parent = this.closest('.section');
      let matchId = parent.id
      let stat = this.getAttribute('stats')
      if (stat==="matches") {
        location.assign(`/${country}/${leagueId}/match?id=${matchId}`)
      } else {
        location.assign(`/${country}/${leagueId}/match?id=${matchId}&stats=${stat}`)
      }
    };
  
</script>

