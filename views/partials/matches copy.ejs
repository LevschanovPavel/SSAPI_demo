<div id="matches-tab" class="col s12">
  <!-- MATCHES -->
  <div class="today_matches_container">
      <% for(let t=0; t < todayMatches.length; t++) { %>
        <div class="event_header">
          <div class="icon--flag event__title fl_<%= todayMatches[t].num %>">
            <div class="event__titleBox">
              <span class="event__title--type"><%= todayMatches[t].country %> </span>
              <span class="event__title--name"><%= todayMatches[t].league %> </span>
            </div>
          </div>
        </div>
        <ul class="collapsible">
              <% for(let tm=0; tm < todayMatches[t].matches.length; tm++) { %>
                <%  if (tm===0) {%>
                  <li class="active">
                <%} else {  %>
                  <li>
                <%}%>
                  <div class="collapsible-header">

                    <div class="event" data-aos="zoom-in">
                      <div class="time">
                        <%= todayMatches[t].matches[tm].matchData %> 
                        <%= todayMatches[t].matches[tm].matchTime %>
                      </div>
                      <div class="home_team">
                        <img class="logo" src="https://www.soccer24.com/res/image/data/<%= todayMatches[t].matches[tm].homeTeamIco %>">
                        <span><%= todayMatches[t].matches[tm].homeTeam %></span>
                      </div>
                      <div class="split"> - </div>
                      <div class="away_team">
                        <span><%= todayMatches[t].matches[tm].awayTeam %></span>
                        <img class="logo" src="https://www.soccer24.com/res/image/data/<%= todayMatches[t].matches[tm].awayTeamIco %>">
                      </div>
                      <div class="iconStats" >
                        <i class="material-icons" linkId='<%= todayMatches[t].matches[tm].matchId %>' title="Statistics">assessment</i>
                      </div>    
                    </div>
                  </div>
                  <div class="collapsible-body">
                    <div class="infoWrapper" data-aos="zoom-in-right" data-aos-delay="300">
                      <div class="odds">
                        <div class="oddsTitle">Odds:</div>
                        <div class="cellWrapper">
                          <span class="cell">
                            <span class="oddsType">1</span>
                            <span class="oddsValue"><%= todayMatches[t].matches[tm].odds.w %></span>
                          </span>
                        </div>
                        <div class="cellWrapper">
                          <span class="cell">
                            <span class="oddsType">X</span>
                            <span class="oddsValue"><%= todayMatches[t].matches[tm].odds.d %></span>
                          </span>
                        </div>
                        <div class="cellWrapper">
                          <span class="cell">
                            <span class="oddsType">2</span>
                            <span class="oddsValue"><%= todayMatches[t].matches[tm].odds.l %></span>
                          </span>
                        </div>                        
                      </div>
                    </div>
                    <div class="infoWrapper">
                      <div class="matchInfo" data-aos="zoom-in-left" data-aos-delay="300">
                        <span>Referee:</span> <%= todayMatches[t].matches[tm].referee %> </br>
                        <span>Venue:</span> <%= todayMatches[t].matches[tm].venue %>
                      </div>
                    </div>                    

                    <div class="container" >
                      <div class="section" id='<%= todayMatches[t].matches[tm].matchId %>'>
                        <div class="header center teal-text text-lighten-2 hide-on-small-only"  data-aos="fade-up">Show Match Statistics</div>
                                                 
                        <!--   Icon Section   -->
                        <div class="row show-on-small hide-on-med-and-up">
                          
                          <div class="col s3 m2 offset-m2">
                            <div class="icon-stats" stats="matches" data-aos="fade-right" data-aos-delay="600">
                              <img src="/img/icon/matches.png" alt="Last matches">
                              <span>Matches</span>
                            </div>
                          </div>
                   
                          <div class="col s3 m2">
                            <div class="icon-stats" stats="goals" data-aos="fade-down" data-aos-delay="600">
                              <img src="/img/icon/goals.png" alt="Total">
                              <span>Total</span>
                            </div>
                          </div>
                    
                          <div class="col s3 m2">
                            <div class="icon-stats" stats="cornerKicks" data-aos="fade-up" data-aos-delay="600">
                              <img src="/img/icon/corner.png" alt="Corners kicks">
                              <span>Corners kicks</span>
                            </div>
                          </div>        
                    
                          
                          <div class="col s3 m2">
                            <div class="icon-stats" stats="fouls" data-aos="fade-right" data-aos-delay="600">
                              <img src="/img/icon/fouls.png" alt="Fouls">
                              <span>Fouls</span>
                            </div>                            
                          </div>
                        </div>    
                        
                        <div class="all-stats center teal-text text-lighten-2 show-on-small hide-on-med-and-up"  data-aos="fade-up" data-aos-delay="600">Show all statistics</div>                   

                        <div class="row hide-on-small-only">
                          <div class="col s3 m2 offset-m2">
                            <div class="icon-stats" stats="matches" data-aos="fade-right" data-aos-delay="600">
                              <img src="/img/icon/matches.png" alt="Last matches">
                              <span>Matches</span>
                            </div>
                          </div>
                   
                          <div class="col s3 m2">
                            <div class="icon-stats" stats="goals" data-aos="fade-down" data-aos-delay="600">
                              <img src="/img/icon/goals.png" alt="Total">
                              <span>Total</span>
                            </div>
                          </div>
                    
                          <div class="col s3 m2">
                            <div class="icon-stats" stats="shotsOnGoal" data-aos="fade-up" data-aos-delay="600">
                              <img src="/img/icon/shots.png" alt="Shots">
                              <span>Shots</span>
                            </div>
                          </div>        
                    
                          
                          <div class="col s3 m2">
                            <div class="icon-stats" stats="cornerKicks" data-aos="fade-right" data-aos-delay="600">
                              <img src="/img/icon/corner.png" alt="Corners kicks">
                              <span>Corners kicks</span>
                            </div>
                          </div>
                        </div>  

                        <div class="row hide-on-small-only">
                          <div class="col s4 m2 offset-m1 offset-s2">

                            <div class="icon-stats" stats="ballPossession" data-aos="fade-left" data-aos-delay="600">
                              <img src="/img/icon/possession.png" alt="Ball possession">
                              <span>Ball possession</span>
                            </div>

                          </div>

                          <div class="col s4 m2 ">
                            <div class="icon-stats" stats="yellowCards" data-aos="fade-left" data-aos-delay="600">
                              <img src="/img/icon/yellow.png" alt="Yellow cards">
                              <span>Yellow cards</span>
                            </div>
                          </div>
                    
                          <div class="col s4 m2">
                            <div class="icon-stats" stats="offsides" data-aos="fade-down" data-aos-delay="600">
                              <img src="/img/icon/offsides.png" alt="Offsides">
                              <span>Offsides</span>
                            </div>
                          </div>
                    
                          <div class="col s4 m2">
                            <div class="icon-stats" stats="penalties" data-aos="fade-up" data-aos-delay="600">
                              <img src="/img/icon/penalties.png" alt="Penalties">
                              <span>Penalties</span>
                            </div>
                          </div>        
                    

                          <div class="col s4 m2">
                            <div class="icon-stats" stats="fouls" data-aos="fade-left" data-aos-delay="600">
                              <img src="/img/icon/fouls.png" alt="Fouls">
                              <span>Fouls</span>
                            </div>
                          </div>
                        </div>
                             
                      </div>
                    </div> 


                  </div>
                  </li>
              <% } %>  
        </ul>
      <% } %> 
  </div>
</div>

