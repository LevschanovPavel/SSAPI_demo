<script>
  // var events = document.getElementsByClassName('event');
  let events = document.getElementsByClassName('icon-stats');
  let allStats = document.getElementsByClassName('all-stats');
  let iconStats = document.querySelectorAll('.iconStats>.material-icons');
  let leagueId = '<%= leagueId %>';
  let country = '<%= country %>';
  for(var i = 0; i < iconStats.length; i++)
    iconStats[i].onclick = function() {
      let matchId = this.getAttribute('linkId')
      location.assign(`/${country}/${leagueId}/match?id=${matchId}`)
    }

  for(var i = 0; i < allStats.length; i++)
    allStats[i].onclick = function() {
      let parent = this.closest('.section');
      let matchId = parent.id
      location.assign(`/${country}/${leagueId}/match?id=${matchId}`)
    }

  for(var i = 0; i < events.length; i++)
    events[i].onclick = function() {
      let parent = this.closest('.section');
      let matchId = parent.id
      let stat = this.getAttribute('stats')
      if (stat==="matches") {
        location.assign(`/${country}/${leagueId}/match?id=${matchId}`)
      } else {
        location.assign(`/${country}/${leagueId}/match?id=${matchId}&stats=${stat}`)
      }
    };
  
</script>